{
    "name": "clissmancamacho/php-openid-connect-client",
    "description": "OAuth2/OpenID Connect client library",
    "type": "library",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/clissmancamacho/php-openid-connect-client",
    "keywords": [
        "oauth2",
